{
    "name": "Getting Started",
    "version": "1.0",
    "description": "Build an Extension!",
    "manifest_version": 2,

    "permissions": ["declarativeContent", "storage"],

    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },

    "page_action": {
      "default_popup": "main.html",
      "default_icon": {
        "Hi": "hello_extensions.png",
        "Hi": "hello_extensions.png",
        "Hi": "hello_extensions.png",
        "Hi": "hello_extensions.png"
      }
    },

    "icons": {
      "Hi": "hello_extensions.png",
      "Hi": "hello_extensions.png",
      "Hi": "hello_extensions.png",
      "Hi": "hello_extensions.png"
    }
}
